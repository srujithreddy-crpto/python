a=int(input("enter the number"))
if a>0:
    for i in range(2,a):
        if a%i==0:
            print("not a prime number",a)
            break
    else:
        print("a is prime number",a)
else:
    print("a is not prime number",a)
    sum1=0
n1=int(input("enter the number"))
while n1>0:
    sum1=sum1+n1
    n1=n1-1
    
print("sum is:",sum1)
    
    sum1=0
n1=int(input("enter the number"))
while n1>0:
    sum1=sum1+n1
    n1=n1-1
    
print("sum is:",sum1)
